{
  "name": "nmrb-daily-github-io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "vite build && git add . && git commit -m \"Deploy: $(date)\" && git push origin main",
    "deploy:force": "vite build && git add . && git commit -m \"Force deploy: $(date)\" --allow-empty && git push origin main",
    "deploy:message": "npm run build && git add . && git commit -m",
    "check:build": "npm run build && echo 'Build successful!'",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build"
  },
  "dependencies": {
    "firebase": "^12.5.0",
    "loglevel": "^1.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "sass": "^1.94.2",
    "vite": "^4.5.5"
  }
}
